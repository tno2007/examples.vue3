    "@grapoza/vue-tree": "^5.1.0",
    "fecha": "^4.2.3",
    "get-value": "^3.0.1",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue3-tree-vue": "^1.0.20"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
